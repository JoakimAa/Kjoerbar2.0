<?xml version="1.0" encoding="utf-8"?>
<rotate
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="270"
    android:toDegrees="270">

    <shape
        android:innerRadiusRatio="@dimen/session_timer_inner_radius"
        android:shape="ring"
        android:thickness="@dimen/session_timer_thickness"
        android:useLevel="true">

        <gradient
            android:angle="0"
            android:endColor="@color/primaryColor"
            android:startColor="@color/primaryLightColor"
            android:type="sweep"
            android:useLevel="false" />

    </shape>

</rotate>